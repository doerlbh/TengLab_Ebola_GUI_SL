% Ebola Modeling
% Data Fitting

% % To Do:
% 1. Open Excel
% 2. Read Data
% 3. Plot Data
% 4. Fit Model
% 5. Fit Parameters
% 6. Plot Model and Data

clear all;
close all;

filename = '/Users/DoerLBH/Dropbox/git/TengLab_Ebola_GUI_SL/Table1_for_code.xlsx';
xlRange = 'A2:E18';
GUI = xlsread(filename,1,xlRange);
SL  = xlsread(filename,2,xlRange);

t = 1:17;
GCase  = GUI(:, 2);
SCase  =  SL(:, 2);
GDeath = GUI(:, 3);
SDeath =  SL(:, 3);
GInf   = GUI(:, 4);
SInf   =  SL(:, 4);
GCFR   = GUI(:, 5);
SCFR   =  SL(:, 5);

fig1 = figure;
plot(t, GInf,'LineWidth', 2); hold on
plot(t, SInf,'LineWidth', 2);
plot(t, GDeath,'LineWidth', 2);
plot(t, SDeath,'LineWidth', 2);
title('Infectious & Death in GUI and SL');
xlabel('month');
ylabel('number');
legend('GInf','SInf', 'GDeath', 'SDeath','Location','northwest');
% close(fig1);

% For GUI

% Initial conditions: 1997 susceptible, 2 infected, 0 recovered? 2000
% population
y0 = [1997 3 0 2000];

% tspan will hold the times for which to solve the system
% of ODEs. We want to solve it at the times that
% correspond to our data
tspan = t;

% Try solving the system at a guess for good parameter values
p0 = [.01 .1];

Gp_opt = sir_optimize(GInf.', tspan, y0, p0);

[t,Gy] = ode45(@sir_ode, tspan, y0, [], Gp_opt);

fig2 = figure;
plot(t, Gy, 'linewidth', 2);
hold on;
plot(t, GInf, 'k*', 'markersize', 10);
legend('Pre-S', 'Pre-I', 'Pre-R', 'GInf', 'Location', 'northwest');
set(gca, 'FontSize', 15);
% close(fig2);


% For SL

% Initial conditions: 1000 susceptible, 3 infected, 0 recovered
y0 = [9997 3 0 2000];

% tspan will hold the times for which to solve the system
% of ODEs. We want to solve it at the times that
% correspond to our data
tspan = t;

% Try solving the system at a guess for good parameter values
p0 = [.01 .1];

Sp_opt = sir_optimize(SInf.', tspan, y0, p0);

[t,Sy] = ode45(@sir_ode, tspan, y0, [], Sp_opt);

fig3 = figure;
plot(t, Sy, 'linewidth', 2);
hold on;
plot(t, SInf, 'k*', 'markersize', 10);
legend('Pre-S', 'Pre-I', 'Pre-R', 'SInf', 'Location', 'northwest');
set(gca, 'FontSize', 15);
% close(fig3);
